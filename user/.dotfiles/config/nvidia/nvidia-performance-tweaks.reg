; === Additional Performance ===
"RMSupportUserdMapDma"=dword:00000001
"RmIgnoreHulkErrors"=dword:00000001
"RmDisableFanDiag"=dword:00000001
"RMG5xL2VidmemPromote"=dword:00000001

; === P-State Lock (Force Maximum Performance) ===
"DisableDynamicPstate"=dword:00000001
"PerfLevelSrc"=dword:00002222
"PowerMizerEnable"=dword:00000001
"PowerMizerLevel"=dword:00000001
"PowerMizerLevelAC"=dword:00000001

; === HDCP Disable (improves latency, breaks DRM content) ===
"RMHdcpKeyglobZero"=dword:00000001

; === GPU Firmware ===
"EnableGpuFirmware"=dword:00000001
"UsePageAttributeTable"=dword:00000001
"InitializeSystemMemoryAllocations"=dword:00000000

