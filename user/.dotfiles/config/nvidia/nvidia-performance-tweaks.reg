Windows Registry Editor Version 5.00

; ============================================================
; NVIDIA Performance Optimization Registry Tweaks
; ============================================================
; Consolidated from multiple experimental tweak files
; Apply these tweaks for maximum gaming/performance
; Reboot required after applying
; ============================================================

; ============================================================
; GPU Driver Performance Settings (0000 = First GPU)
; ============================================================
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Class\{4d36e968-e325-11ce-bfc1-08002be10318}\0000]

; === Memory Management ===
"WDDMv21Enable2MPageSupport"=dword:00000001
"WDDMv21Enable64KbSysmemSupport"=dword:00000001
"WDDMv2SupportPageTableInSystemMemory"=dword:00000001
"PreferSystemMemoryContiguous"=dword:00000001
"CPUVisibleHeap"=dword:00000001

; === Display Features ===
"EnableIndependentFlip"=dword:00000001
"EnableDirectFlip"=dword:00000001
"EnableDirectFlipFor3dLut"=dword:00000001

; === Performance Modes ===
"AllowMaxPerf"=dword:00000001
"EnablePerformanceMode"=dword:00000001
"EnableAggressivePStateBoost"=dword:00000001

; === Multi-Threading ===
"RMEnableMultiThreading"=dword:00000001
"RmGpsPsEnablePerCpuCoreDpc"=dword:00000001
"RMDisablePerIntrDPCQueueing"=dword:00000001

; === Cache & Memory Optimizations ===
"EnableTLBInvalidateOpt"=dword:00000001
"RmPVMRL"=dword:00000001
"RMGpuCacheEnable"=dword:00000001
"RMIMPDynamicMempoolAllocation"=dword:00000001

; === Power & Clock Management ===
"RMUseAggressiveGC6"=dword:00000001
"EnableFastGC6"=dword:00000001
"AllowHWGC6"=dword:00000001
"RmClkPowerOffDramPllWhenUnused"=dword:00000000
"FastResume"=dword:00000001

; === Audio (Disable D3Cold for lower latency) ===
"EnableHDAudioD3Cold"=dword:00000000

; === Advanced Optimizations ===
"RMLockingMode"=dword:00000001
"RMIntrLockingMode"=dword:00000001
"EnableRS4ContextlessPresent"=dword:00000001
"EnableSparseTexture"=dword:00000001
"EnableSegmentPreservation"=dword:00000001
"EnableAllocationMigration"=dword:00000001

; === Security/Telemetry Reduction ===
"RmValidateClientData"=dword:00000000
"RMUcodeEncryption"=dword:00000000
"RMEnableEventTracer"=dword:00000000
"EnableNVLinkL2Tracking"=dword:00000000
"RmLockTimeCollect"=dword:00000000
"RMCtxswLog"=dword:00000000

; === DPC & Priority ===
"RM654663"=dword:00000001
"RMEnableForceSharedLock"=dword:00000001
"RMPriorityBoost"=dword:00000001
"RmStreamMemOps"=dword:00000001

; === Resource Management ===
"RMClientListDeferredFree"=dword:00000001
"RMClientListDeferredFreeLimit"=dword:00000001
"RmForceGridDisplayless"=dword:00000001
"RMForcePcieConfigSave"=dword:00000001
"RMForceEnableDisplayMScg"=dword:00000001
